    "react-addons-test-utils": "^15.4.0",
    "react-dev-utils": "^0.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "serverless": "^1.2.1",
    "serverless-webpack": "^1.0.0-rc.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "sw-precache-webpack-plugin": "^0.5.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "webpack-node-externals": "^1.5.4",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "apollo-client": "^0.5.6",
    "auth0": "^2.5.0",
    "auth0-js": "^7.6.0",
    "formidable": "^1.0.17",
    "graphql": "^0.8.2",
    "graphql-tag": "^1.0.0",
    "joi": "^10.0.1",
    "jwt-decode": "^2.1.0",
    "material-ui": "^0.16.1",
    "material-ui-chip-input": "^0.9.1",
    "react": "^15.4.0",
    "react-apollo": "^0.6.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.0",
    "react-test-renderer": "^15.4.0",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0-alpha7",
    "vogels": "^2.2.0",
    "vogels-promisified": "^2.2.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:prod": "PROD_API=true npm start",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "deploy:api:dev": "serverless deploy",
    "deploy:api:prod": "serverless deploy --stage production",
    "deploy:web:staging": "netlify deploy -e staging -t $NETLIFY_TOKEN",
    "deploy:web:prod": "netlify deploy -e production -t $NETLIFY_TOKEN",
    "graphiql": "node ./api/dev/server-setup.js",
    "coverage": "npm test -- --coverage",
    "codecov": "npm run coverage && codecov"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js",
      "^.+\\.(gql|graphql)$": "<rootDir>/config/jest/gqlStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": 0,
      "import/no-extraneous-dependencies": 0,
      "no-underscore-dangle": 0,
      "import/no-named-as-default": 0
    },
    "env": {
      "jest": true,
      "browser": true
    }
  }
}
