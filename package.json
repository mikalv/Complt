    "mockdate": "^2.0.1",
    "pouch-redux-middleware": "https://github.com/mitchellhamilton/pouch-redux-middleware",
    "pouchdb-adapter-http": "^6.1.2",
    "pouchdb-adapter-idb": "^6.1.2",
    "pouchdb-adapter-memory": "^6.1.2",
    "pouchdb-adapter-websql": "^6.1.2",
    "pouchdb-core": "^6.1.2",
    "pouchdb-memory": "^6.0.0",
    "pouchdb-replication": "^6.1.2",
    "preact": "^8.1.0",
    "preact-compat": "^3.15.0",
    "preact-css-transition-group": "^1.1.1",
    "preact-material-components": "^1.0.4",
    "preact-redux": "^2.0.1",
    "preact-router": "^2.5.2",
    "raven-js": "^3.14.2",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.3",
    "react-sortable-hoc": "https://github.com/clauderic/react-sortable-hoc",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-persist": "^4.6.0",
    "redux-thunk": "^2.2.0",
    "sugar-date": "^2.0.4",
    "unfetch": "^2.1.2",
    "uuid": "^3.0.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "node scripts/build.js",
    "build:electron": "REACT_APP_ELECTRON=true node scripts/build.js && cd build && yarn && cd .. && electron-packager build/ Complt --platform=darwin --arch=x64 --icon=build/Icon.icns --out=dist/macos",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "test:watch": "node scripts/test.js --env=jsdom --verbose --watch",
    "test:e2e": "wdio",
    "deploy:web:landing": "netlify deploy -e landing -t $NETLIFY_TOKEN",
    "deploy:web:staging": "netlify deploy -e staging -t $NETLIFY_TOKEN",
    "deploy:web:prod": "netlify deploy -e production -t $NETLIFY_TOKEN",
    "deploy:exponent": "exp login -u $EXPONENT_USERNAME -p $EXPONENT_PASSWORD && exp publish",
    "coverage": "npm test -- --coverage",
    "codecov": "CI_BUILD_REPO=true npm run coverage && CI_BUILD_REPO=true codecov"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?redux-persist|react-sortable-hoc)"
    ],
    "moduleNameMapper": {
      "(.*)PouchDB(.*)": "pouchdb-memory",
      "react-native": "react-native-mock",
      "(.*)react-sortable-hoc(.*)": "<rootDir>/src/test/HOC.js",
      "react-router-dom/Link": "<rootDir>/src/test/LinkMock.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "collectCoverageFrom": [
      "src/common/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
