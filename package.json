    "netlify-cli": "^1.1.0",
    "node-sass": "^4.0.0",
    "object-assign": "4.1.1",
    "offline-plugin": "^4.6.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "pushstate-server": "^2.2.1",
