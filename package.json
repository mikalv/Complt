    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "auth0-js": "^7.6.0",
    "jwt-decode": "^2.1.0",
    "mockdate": "^2.0.1",
    "pouch-redux-middleware": "^0.5.0",
    "pouchdb": "^6.1.1",
    "pouchdb-adapter-memory": "^6.1.1",
    "pouchdb-memory": "^6.0.0",
    "raven-js": "^3.9.1",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-transition-group": "^15.4.1",
    "react-dom": "^15.4.0",
    "react-icons": "^2.2.3",
    "react-md": "^1.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-test-renderer": "^15.4.0",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-persist": "^4.0.0-alpha7",
    "redux-raven-middleware": "^1.2.0",
    "redux-thunk": "^2.1.0",
    "uuid": "^3.0.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "lint": "eslint . --ext .js --ext .gql",
    "build": "node scripts/build.js",
    "build:sentry": "SENTRY=true node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "test:watch": "node scripts/test.js --env=jsdom --verbose --watch",
    "deploy:web:staging": "netlify deploy -e staging -t $NETLIFY_TOKEN",
    "deploy:web:prod": "netlify deploy -e production -t $NETLIFY_TOKEN",
    "coverage": "npm test -- --coverage",
    "codecov": "npm run coverage && codecov"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js",
      "pouchdb/lib/index-browser": "pouchdb-memory"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/",
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
