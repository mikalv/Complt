    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "http-proxy-middleware": "0.17.4",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "20.0.4",
    "jest-fetch-mock": "^1.2.1",
    "json-loader": "0.5.7",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cli": "^1.2.2",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "offline-plugin": "^4.8.3",
    "postcss-loader": "1.3.3",
    "preload-webpack-plugin": "^1.2.3",
    "prettier": "^1.5.3",
    "pushstate-server": "^3.0.1",
    "recursive-readdir": "2.2.1",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "saucelabs": "^1.4.0",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "wdio": "^0.3.3",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-sauce-service": "^0.4.0",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.8.0",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "2.6.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "auth0-js": "^8.8.0",
    "classnames": "^2.2.5",
    "ganalytics": "^2.0.1",
    "linkstate": "^1.0.1",
    "mockdate": "^2.0.1",
    "pouch-redux-middleware": "https://github.com/mitchellhamilton/pouch-redux-middleware",
    "pouchdb-adapter-http": "^6.3.4",
    "pouchdb-adapter-idb": "^6.3.4",
    "pouchdb-adapter-memory": "^6.3.4",
    "pouchdb-adapter-websql": "^6.3.4",
    "pouchdb-core": "^6.3.4",
    "pouchdb-memory": "^6.0.0",
    "pouchdb-replication": "^6.3.4",
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0",
    "preact-css-transition-group": "^1.3.0",
    "preact-material-components": "^1.1.0",
    "preact-redux": "^2.0.2",
    "preact-router": "^2.5.5",
    "react-icons": "^2.2.5",
    "react-sortable-hoc": "https://github.com/clauderic/react-sortable-hoc",
    "redux": "^3.7.2",
    "redux-persist": "^4.8.2",
    "redux-thunk": "^2.2.0",
    "sugar-date": "^2.0.4",
    "tinytime": "^0.2.6",
    "unfetch": "^3.0.0",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dashboard -- webpack-dev-server --config config/webpack.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "NODE_ENV=production webpack --progress --env.production --config config/webpack.config.js --display-modules --display-optimization-bailout",
    "test": "jest --verbose",
    "test:watch": "jest --coverage --verbose --watch",
    "test:e2e": "wdio",
    "deploy:web:landing": "netlify deploy -e landing -t $NETLIFY_TOKEN",
    "deploy:web:staging": "netlify deploy -e staging -t $NETLIFY_TOKEN",
    "deploy:web:prod": "netlify deploy -e production -t $NETLIFY_TOKEN",
    "coverage": "npm test -- --coverage",
    "codecov": "CI_BUILD_REPO=true npm run coverage && CI_BUILD_REPO=true codecov"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?redux-persist|react-sortable-hoc)"
    ],
    "moduleNameMapper": {
      "(.*)PouchDB(.*)": "pouchdb-memory",
      "react-native": "react-native-mock",
      "(.*)react-sortable-hoc(.*)": "<rootDir>/src/test/HOC.js",
      "react-router-dom/Link": "<rootDir>/src/test/LinkMock.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "collectCoverageFrom": [
      "src/common/**/*.{js,jsx}"
    ]
  }
}
