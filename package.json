    "exponent": "^14.0.0",
    "history": "^4.6.0",
    "jwt-decode": "^2.1.0",
    "menubar": "^5.2.0",
    "mockdate": "^2.0.1",
    "native-base": "^2.0.7",
    "pouch-redux-middleware": "^0.6.0",
    "pouchdb": "^6.1.1",
    "pouchdb-adapter-memory": "^6.1.1",
    "pouchdb-memory": "^6.0.0",
    "pouchdb-react-native": "^6.1.26",
    "raven-js": "^3.12.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-transition-group": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-form": "^1.2.6",
    "react-icons": "^2.2.3",
    "react-md": "1.0.6",
    "react-native": "https://github.com/exponent/react-native/archive/sdk-14.0.0.tar.gz",
    "react-native-material-ui": "^1.6.0",
    "react-navigation": "^1.0.0-beta.1",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.6.0",
    "redux-persist": "^4.4.2",
    "redux-raven-middleware": "^1.2.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.0.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "lint": "eslint . --ext .js --ext .gql",
    "build": "node scripts/build.js",
    "build:electron": "REACT_APP_ELECTRON=true node scripts/build.js && cd build && yarn && cd .. && electron-packager build/ Complt --platform=darwin --arch=x64 --icon=build/Icon.icns --out=dist/macos",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "test:watch": "node scripts/test.js --env=jsdom --verbose --watch",
    "test:e2e": "wdio",
    "deploy:web:landing": "netlify deploy -e landing -t $NETLIFY_TOKEN",
    "deploy:web:staging": "netlify deploy -e staging -t $NETLIFY_TOKEN",
    "deploy:web:prod": "netlify deploy -e production -t $NETLIFY_TOKEN",
    "deploy:exponent": "exp login -u $EXPONENT_USERNAME -p $EXPONENT_PASSWORD && exp publish",
    "coverage": "npm test -- --coverage",
    "codecov": "npm run coverage && codecov"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|exponent|@exponent/*|react-navigation)"
    ],
    "moduleNameMapper": {
      "pouchdb/lib/index-browser": "pouchdb-memory",
      "react-native": "react-native-mock",
      "react-router-dom/Link": "<rootDir>/src/test/LinkMock.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "collectCoverageFrom": [
      "src/web/**/*.{js,jsx}",
      "src/common/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
